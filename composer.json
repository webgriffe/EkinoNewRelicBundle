{
    "name": "webgriffe/newrelic-bundle",
    "type": "symfony-bundle",
    "description": "Integrate New Relic into Symfony2",
    "keywords": ["metric", "performance", "monitoring"],
    "homepage": "https://github.com/webgriffe/EkinoNewRelicBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@ekino.com",
            "homepage": "http://ekino.com"
        },
        {
            "name": "Webgriffe SRL",
            "email": "support@webgriffe.com",
            "homepage": "https://www.webgriffe.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "symfony/config": "^8.0",
        "symfony/console": "^8.0",
        "symfony/dependency-injection": "^8.0",
        "symfony/event-dispatcher": "^8.0",
        "symfony/http-kernel": "^8.0",
        "monolog/monolog": "^3.0|^4.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^6.0",
        "symfony/framework-bundle": "^8.0",
        "symfony/phpunit-bridge": "^8.0",
        "twig/twig": "^3.0",
        "symfony/monolog-bundle": "^4.0"
    },
    "autoload": {
        "psr-4": { "Ekino\\NewRelicBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": { "Ekino\\NewRelicBundle\\Tests\\": "Tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
